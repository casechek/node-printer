{
  "name": "@casechek/node-printer",
  "description": "Node.js printer bindings",
  "version": "0.6.2",
  "homepage": "https://github.com/casechek/node-printer",
  "author": {
    "name": "Ion Lupascu",
    "url": "http://program-support.co.uk/",
    "email": "ionlupascu@gmail.com"
  },
  "contributors": [
    "Stephen Carlin <stephen@casechek.com>",
    "Steven Lehn <steven.lehn@casechek.com>",
    "Thiago Lugli <thiagoelg@gmail.com>",
    "Eko Eryanto <ekoeryanto@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/casechek/node-printer.git"
  },
  "devDependencies": {
    "nodeunit": "^0.11.3",
    "prebuild": "^13.0.1",
    "prebuild-ci": "^4.0.0",
    "node-abi": "^3.56.0",
    "patch-package": "^8.0.0"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "scripts": {
    "install": "npm run apply-patches && prebuild-install || node-gyp rebuild",
    "apply-patches": "patch-package",
    "prebuild-all": "prebuild --all --force --strip --verbose --openssl_fips=X",
    "prebuild": "prebuild",
    "prebuild-electron": "prebuild --runtime electron --target 33.0.0 --strip --verbose",
    "rebuild": "node-gyp rebuild",
    "test": "nodeunit test"
  },
  "binary": {
    "module_name": "node_printer",
    "module_path": "./lib/",
    "host": "https://github.com/casechek/node-printer/releases/download/",
    "remote_path": "v{version}"
  },
  "license": "MIT",
  "main": "./lib/printer",
  "dependencies": {
    "nan": "^2.22.2",
    "prebuild-install": "^7.1.1"
  },
  "types": "types/index.d.ts"
}
